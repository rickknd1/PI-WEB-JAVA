<?xml version="1.0" encoding="UTF-8"?>


        <?import javafx.scene.control.*?>
        <?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.syncylinky.controllers.PostController" spacing="10" alignment="TOP_CENTER">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>

    <Label fx:id="alertTitle" styleClass="alert-title"/>
    <Label fx:id="alertMessage" wrapText="true" styleClass="alert-message"/>
    <Button text="OK" onAction="#closeAlert"/>
</VBox>